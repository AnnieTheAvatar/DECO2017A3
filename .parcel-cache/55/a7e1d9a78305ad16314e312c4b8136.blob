/*pomodoro*/

@import url("https://fonts.googleapis.com/css?family=Roboto:300,400,500&display=swap");

* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}

.pomodoro-container {
  font-family: "Roboto", sans-serif;
  font-size: 16px;
  text-align: center;
}

.progressbar-text {
  font-family: "Lobster", cursive;
  font-size: 40px;
}

.session-type {
  font-size: 1.8rem;
  font-weight: 400;
  padding-bottom: 1.25rem;
}

.timer-display {
  font-size: 3rem;
}

.button-group {
  list-style: none;
  padding: 0;
  display: flex;
  justify-content: center;
  font-size: 1.25rem;
}

#pomodoro-sessions {

  height: 0px;
}

.button {
  background: #8390Fa;
  color: pink;
  width: 2.5rem;
  height: 2.5rem;
  border-radius: 100%;
  cursor: pointer;
}

.button i {
  margin-top: 0.7rem;
}

li.stop {
  margin-left: 0.5rem;
}

/* Media Queries */

@media (max-width: 40rem) {
  .timer-display {
    font-size: 2rem;
  }
}

.hidden {
  display: none;
}

iframe {
  display:block;
  margin: auto;
}

.UI {
  background-color: lightcoral;
}

.NI {
  background-color: mistyrose;
}

.UN {
  background-color: mintcream;
}

.NN {
  background-color: snow;
}

.blank th {
  width:20%;
}

.urgent th {
  width:40%
}