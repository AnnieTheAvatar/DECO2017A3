
* {
  font-family: 'Open Sans', sans-serif;
}

body {
  background: url('b914efa579e87be556a97dd9e9500fca') no-repeat center center fixed; 
  -webkit-background-size: cover;
  -moz-background-size: cover;
  -o-background-size: cover;
  background-size: cover;
  }

::-webkit-scrollbar {
  width: 10px;
}

/* Track */

::-webkit-scrollbar-track {
  opacity: 0;
}

/* Handle */

::-webkit-scrollbar-thumb {
  background: lightpink;
}

/* Handle on hover */

::-webkit-scrollbar-thumb:hover {
  background: lightcoral;
}

.header {
  text-align: center;
  top: 10px; 
  width: 100%; 
  z-index: 1;
  padding-bottom: 10px;
  margin-bottom: 5%;
  background: pink;
}

.header h1 {
  font-size: 50px;
}

h1 {
  text-align: center;
  font-family: 'Lobster', cursive;
  font-size: 30px;
  margin-bottom: 10px;
}

.column {
  float: left;
  width: 30%;
  margin:10px;
}

.column-left {
  float: left;
  width: 30%;
  height: 100%;
  margin-top: 10px;
  margin-left: 10px;
}

.row:after {
  content: "";
  display: table;
  clear: both;
}

.row {
  margin-left: 20%;
  margin-right: 12%;
}

.scrollable {
    height: 600px;
    overflow-y: auto;
}

.music-phone {
  transform: rotate(-3deg);
  position: fixed;
  top: 150px;
  left: 50px;
  
}

.music {
  border:2px solid black;
    position: fixed;
    background-color:black;
    width:250px;
    height:450px;
    border-radius: 20px; 
    box-shadow: 5px 5px 7px rgba(33,33,33,.7)  
}

@media only screen and (max-width: 1500px) {
  .music-phone{
    transform: rotate(0);
    position: static;
    padding: 5%;
  }
  .music {
    position: static;
    width: 100%;
    height: 300px;
    border: 0;
    border-radius: 0;
    background: 0;
  }
}

@media only screen and (max-width: 1000px) {
  .column {
    width: 53%;
    float: right;
  }
  .column-left {
    width: 40%;
  }
  .row {
    margin: 5px;
  }
  .header {
    position: static;
  }
  body {
    background: url('abf38feb553c73a712f9cc16df9cff3b') no-repeat top center;
  -webkit-background-size: cover;
  -moz-background-size: cover;
  -o-background-size: cover;
  background-size: cover;
  }
  .scrollable {
  height: 100%;
}
}

@media only screen and (max-width: 600px) {
  .column, .column-left {
    width: 95%;
    float: left;
  }
  .music {
  height: 200px;
}

}

/* Button used to open the contact form - fixed at the bottom of the page */

.open-button {
  background-color: #8390Fa;
  color: white;
  padding: 16px 20px;
  border: none;
  border-radius: 50px;
  cursor: pointer;
  opacity: 0.8;
  margin: 0px;
  width: 50;
  font-size: 20px
}

/* The popup form - hidden by default */

.form-popup, .reading-popup {
  display: none;
  left: 15px;
  border: 3px solid #f1f1f1;
  z-index: 9;
  margin: 10px;
  padding: 10px;
  background-color: pink;
  border-radius: 10px;
}

/* Add styles to the form container */

.form-container {
  max-width: 100px;
  padding: 10px;
  background-color: white;
}

/* Full-width input fields */

.form-container input[type=text], .form-container input[type=date], .form-container input[type=number], .form-container input[type=range]{
  width: 50%;
  padding: 15px;
  margin: 5px 0 22px 0;
  border: none;
  background: #f1f1f1;
}

/* When the inputs get focus, do something */

.form-container input[type=text]:focus, .form-container input[type=date]:focus, .form-container input[type=number]:focus, .form-container input[type=range]:focus{
  background-color: #ddd;
  outline: none;
}

/* Set a style for the button */

.form-container .btn {
  color: white;
  padding: 16px 20px;
  border: none;
  cursor: pointer;
  width: 100%;
  margin-bottom:10px;
  opacity: 0.8;
}

/* Add a red background color to the cancel button */

.form-container .cancel {
  background-color: red;
}

/* Add some hover effects to buttons */

.form-container .btn:hover, .open-button:hover, button:hover {
  opacity: 1;
}

